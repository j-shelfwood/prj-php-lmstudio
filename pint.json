{
  "preset": "laravel",
  "rules": {
    "declare_strict_types": true,
    "void_return": true,
    "strict_comparison": true,
    "strict_param": true,
    "no_unused_imports": true,
    "ordered_imports": true,
    "single_quote": true,
    "trailing_comma_in_multiline": true,
    "no_extra_blank_lines": true,
    "blank_line_before_statement": {
      "statements": [
        "break",
        "continue",
        "declare",
        "return",
        "throw",
        "try",
        "while",
        "for",
        "foreach",
        "if",
        "switch",
        "yield"
      ]
    }
  }
}
